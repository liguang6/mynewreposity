<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.byd.admin.modules.sys.dao.SysTextCookieDao">


	<select id="selectinputValue" resultType="String" parameterType="Map">
		select t.input_value from sys_text_cookie t where t.input_id=#{inputId} and t.user_id=#{userId}
	</select>

</mapper>